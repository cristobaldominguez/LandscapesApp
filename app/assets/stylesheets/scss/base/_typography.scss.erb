@font-face {
    font-family: 'KelsonSans';
    src:url("<%= asset_path 'Kelson_Sans_Light.woff2' %>") format('woff2'),
        url("<%= asset_path 'Kelson_Sans_Light.woff' %>") format('woff'),
        url("<%= asset_path 'Kelson_Sans_Light.otf' %>") format('opentype');
    font-style: normal;
    font-weight: 200;
}

